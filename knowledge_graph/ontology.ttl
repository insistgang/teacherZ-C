@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix kg: <http://example.org/xiaohao-cai-kg#> .

kg:KnowledgeGraph a owl:Ontology ;
    rdfs:label "Xiaohao Cai Paper Knowledge Graph"@en ;
    rdfs:comment "Knowledge graph for Xiaohao Cai's research papers in image processing, machine learning, and related fields"@en ;
    owl:versionInfo "1.0" .

kg:Entity a owl:Class ;
    rdfs:label "Entity"@en .

kg:Paper a owl:Class ;
    rdfs:subClassOf kg:Entity ;
    rdfs:label "Paper"@en ;
    rdfs:comment "Academic paper or publication"@en .

kg:Author a owl:Class ;
    rdfs:subClassOf kg:Entity ;
    rdfs:label "Author"@en ;
    rdfs:comment "Paper author or researcher"@en .

kg:Method a owl:Class ;
    rdfs:subClassOf kg:Entity ;
    rdfs:label "Method"@en ;
    rdfs:comment "Algorithm, model, or technique"@en .

kg:Domain a owl:Class ;
    rdfs:subClassOf kg:Entity ;
    rdfs:label "Domain"@en ;
    rdfs:comment "Application domain or field"@en .

kg:Tool a owl:Class ;
    rdfs:subClassOf kg:Entity ;
    rdfs:label "Tool"@en ;
    rdfs:comment "Software tool, framework, or programming language"@en .

kg:usesMethod a owl:ObjectProperty ;
    rdfs:domain kg:Paper ;
    rdfs:range kg:Method ;
    rdfs:label "uses method"@en ;
    rdfs:comment "Paper uses this method"@en .

kg:appliesToDomain a owl:ObjectProperty ;
    rdfs:domain kg:Paper ;
    rdfs:range kg:Domain ;
    rdfs:label "applies to domain"@en ;
    rdfs:comment "Paper applies to this domain"@en .

kg:inheritsFrom a owl:ObjectProperty ;
    rdfs:domain kg:Method ;
    rdfs:range kg:Method ;
    rdfs:label "inherits from"@en ;
    rdfs:comment "Method inherits from another method"@en .

kg:improves a owl:ObjectProperty ;
    rdfs:domain kg:Method ;
    rdfs:range kg:Method ;
    rdfs:label "improves"@en ;
    rdfs:comment "Method improves upon another method"@en .

kg:writes a owl:ObjectProperty ;
    rdfs:domain kg:Author ;
    rdfs:range kg:Paper ;
    rdfs:label "writes"@en ;
    rdfs:comment "Author writes this paper"@en .

kg:cites a owl:ObjectProperty ;
    rdfs:domain kg:Paper ;
    rdfs:range kg:Paper ;
    rdfs:label "cites"@en ;
    rdfs:comment "Paper cites another paper"@en .

kg:relatedTo a owl:ObjectProperty ;
    rdfs:domain kg:Paper ;
    rdfs:range kg:Paper ;
    rdfs:label "related to"@en ;
    rdfs:comment "Paper is related to another paper"@en ;
    owl:SymmetricProperty true .

kg:usesTool a owl:ObjectProperty ;
    rdfs:domain kg:Paper ;
    rdfs:range kg:Tool ;
    rdfs:label "uses tool"@en ;
    rdfs:comment "Paper uses this tool"@en .

kg:hasTitle a owl:DatatypeProperty ;
    rdfs:domain kg:Paper ;
    rdfs:range xsd:string ;
    rdfs:label "has title"@en .

kg:hasYear a owl:DatatypeProperty ;
    rdfs:domain kg:Paper ;
    rdfs:range xsd:integer ;
    rdfs:label "has year"@en .

kg:hasArxivId a owl:DatatypeProperty ;
    rdfs:domain kg:Paper ;
    rdfs:range xsd:string ;
    rdfs:label "has arXiv ID"@en .

kg:hasVenue a owl:DatatypeProperty ;
    rdfs:domain kg:Paper ;
    rdfs:range xsd:string ;
    rdfs:label "has venue"@en .

kg:hasName a owl:DatatypeProperty ;
    rdfs:domain kg:Entity ;
    rdfs:range xsd:string ;
    rdfs:label "has name"@en .

kg:hasAffiliation a owl:DatatypeProperty ;
    rdfs:domain kg:Author ;
    rdfs:range xsd:string ;
    rdfs:label "has affiliation"@en .

kg:hasCategory a owl:DatatypeProperty ;
    rdfs:domain kg:Method ;
    rdfs:range xsd:string ;
    rdfs:label "has category"@en .

kg:hasDescription a owl:DatatypeProperty ;
    rdfs:domain kg:Entity ;
    rdfs:range xsd:string ;
    rdfs:label "has description"@en .
